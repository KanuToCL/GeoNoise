{
  "name": "@geonoise/engine",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./api": {
      "import": "./dist/api/index.js",
      "types": "./dist/api/index.d.ts"
    },
    "./propagation": {
      "import": "./dist/propagation/index.js",
      "types": "./dist/propagation/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:physics": "vitest run tests/physics-validation.spec.ts",
    "test:physics-report": "PHYSICS_REPORT=true vitest run tests/physics-validation.spec.ts",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint src",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "perf:baseline": "vitest run --reporter=verbose perf/"
  },
  "dependencies": {
    "@geonoise/core": "*",
    "@geonoise/geo": "*",
    "@geonoise/shared": "*"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
